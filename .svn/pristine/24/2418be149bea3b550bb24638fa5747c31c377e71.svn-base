//家校互动动画
@include keyframes(interaction-img1){
  0%{
    @include opacity(0);
    @include transform(scale(1) translate3d(-1000px,0,-1000px));
  }
  100%{
    @include opacity(1);
    @include transform(scale(1) translate3d(0,0,0));
  }
}
@include keyframes(interaction-img2){
  0%{
    @include opacity(0);
    @include transform(scale(1) translate3d(1000px,0,-1000px));
  }
  100%{
    @include opacity(1);
    @include transform(scale(1) translate3d(0,0,0));
  }
}

//忘记修改登录密码
.forgetpassword-box{
  width: 432px;
  height: 630px;
  margin: 80px auto;
  h2{
    margin: 0;
  }
  >p{
    margin-bottom: 50px;
  }
  .select-status{
    a{
      width: 210px;
    }
    a.margin{
      margin-left: 8px;
    }
  }
  form{
    fieldset{
      .verifycode{
        width: 118px;
        height: 50px;
        overflow: hidden;
        line-height: 50px;
        @include inline-block();
        img{
          @include inline-block();
          width: 100%;
        }
      }
      input{
        width: 337px;
        @include box-sizing(border-box);
        padding: 12px 36px;
        font-size: 16px;
        @include border-radius(3px);
        &:focus{
          @extend %transition-03;
          //@include box-shadow(0 0 1px 1px #3bce83);
        }
      }
      .inp-width{
        width: 202px;
        margin-right: 9px;
      }
      .send-code{
        width: 118px;
        height: 49px;
      }
      .relative{
        position: relative;
        >i{
          position: absolute;
          left:11px;
          top:0;
        }
      }
      .forgetpassword-title{
        width: 95px;
        @include inline-block();
      }
      .btn-login{
        height: 50px;
        line-height:40px;
        @include border-radius(3px);
        font-size: 18px;
      }
      em{
        font-style: normal;
        font-size: 16px;
        &.v-error{
          display: block;
          position: absolute;
          right:16px;
          top:-4px;
          color:$danger_color;
        }
      }
      span{
        &.v-error{
          >input{
            @include box-shadow(0 0 1px 1px $danger_color);
          }
        }
      }
      .errorfocus{
        &:focus{
          @include box-shadow(0 0 1px 1px $danger_color);
        }
        ~ span{
          color:$danger_color;
        }
      }
      .successfocus{
        &:focus{
          @include box-shadow(0 0 1px 1px $success-color);
        }
        ~ span{
          color:$success-color;
        }
      }
    }
  }
}

