//视频操作 下载评分分享
.video-operation{
  background-color: #FFF;
  @include box-shadow(0 5px 5px #eaeaea);
  ul{
    li{
      display: inline-block;
    }
  }
  .operation{
    padding: 15px;
    height: 90px;
    @include box-sizing(border-box);
  }
  .download{
    width: 150px;
    border-right: 1px solid $border-color;
    ul{
      li{
        i{
          font-size: 25px;
          color:#444;
          @extend %transition-03;
        }
        a:hover{
          i{
            color:$main_color;
          }
        }
      }
      li.margin{
        margin-right: 20px;
      }
    }
  }
  .share{
    width: 220px;
    border-right: 1px solid $border_color;
    ul{
      li{
        vertical-align: top;
        margin: 0 3px;
        i{
          font-size: 30px;
          color:#7A7A7A;
          @extend %transition-03
        }
        a.qzone{
          &:hover{
            i{
              color:#FECC2F;
            }
          }
        }
        a.sina{
          @include a-color(#E6162D,#E6162D，#E6162D，#E6162D);
          &:hover{
            i{
              color:#E6162D;
            }
            span{
              color:#E6162D;
            }
          }
        }
        a.weixin{
          &:hover{
            i{
              color:#81D741;
            }
          }
        }
      }
    }
  }
  @include keyframes(success){
    to{
      stroke-dasharray: 0;
      stroke-dashoffset: 0;
    }
  }
  .star{
    padding-left: 30px;
    .tips{
      display: none;
      font-style: normal;
      margin-left: 30px;
      >i{
        vertical-align: middle !important;
      }
    }
    .success-tips{
           &,i{
             color:$main_color !important;
           }
       }
    .error-tips{
      &,i{
        color:$danger_color !important;
      }
    }
    .error{
      .star-grade-msg{
        @extend %transition-03;
        color:$danger_color
      }
      rect{
        stroke: $danger_color;
        @include animation(success 1s cubic-bezier(.17,.67,.75,1.11) forwards);

      }
    }
    .success{
      .star-grade-msg{
        @extend %transition-03;
        color:$main_color;
      }
      rect{
        stroke: $main_color;
        @include animation(success 1s cubic-bezier(.17,.67,.75,1.11) forwards);

      }
    }
    div.margin-top{
      margin-top: 5px;
    }
    div{
      span{
        color:#666;
      }
      i{
        cursor: pointer;
        font-size: 18px;
        color:#dcdcdc;
        vertical-align: baseline;
        margin-right: 2px;
      }
      i.active{
        color:#FECC2F;
      }
    }
    div.star-grade{
      position: relative;
      svg{
        position: absolute;
        width: 270px;
        height: 30px;
        left: -9px;
        top: -3px;
        pointer-events: none;
        rect{
          stroke-width: 2;
          width: 270px;
          height: 30px;
          fill: transparent;
          stroke-dasharray: 1500;
          stroke-dashoffset: 1800;
          stroke-linecap: round;
          stroke-linejoin: round;

        }

      }
    }
  }
}

