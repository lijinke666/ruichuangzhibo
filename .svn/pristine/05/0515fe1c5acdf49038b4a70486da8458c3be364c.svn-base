.flex-vc { /* 09版 */ -webkit-box-align: center; -moz-box-align: center; -ms-box-align: center; box-align: center; /* 12版 */ -webkit-align-items: center; -moz-align-items: center; -ms-align-items: center; -o-align-items: center; align-items: center; }

