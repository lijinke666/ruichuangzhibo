//回到顶部
$floatBarBox_height:70px;
.floatBar{
    position: fixed;
    right:0;
    bottom:200px;
    z-index: 9999;
    background-color:#FFF;
    @include box-shadow( -1px 1px 3px 3px #f3f3f3);
    .weixin:hover{
        ~ .floatBar-weixin{
            @include transform(scale(1));
            display: block\9;
        }
    }
    .phone:hover{
        ~ .floatBar-phone{
            @include transform(scale(1));
            display: block\9;
        }
    }
    .floatBar-weixin,.floatBar-phone{
        position: absolute;
        background-color: #FFF;
        @extend %transition-03;
        @include transform(scale(0));
        display: none\9;
        @include transform-origin(right)
    }
    .floatBar-weixin{
        @include box-shadow( 0px 0px 1px 3px #f3f3f3);
        top:-35px;
        padding: 8px;
        left:-198px;
        &:before{
            content: "";
            width: 0;
            height: 0;
            border:15px solid transparent;
            border-left-color:#FFF;
            position: absolute;
            left: 177px;
            bottom: 20px;
        }
    }
    .floatBar-phone{
        top:7px;
        left: -277px;
        padding: 12px;
        @include box-shadow( 0px 0px 1px 3px #f3f3f3);
        &:before{
            content: "";
            width: 0;
            height: 0;
            border:15px solid transparent;
            border-left-color:#FFF;
            position: absolute;
            left: 241px;
            bottom: 11px;
        }
        i{
            vertical-align: middle;
        }
    }
    .floatBar-box{
        text-align: center;
        width:$floatBarBox_height;
        height: $floatBarBox_height;
        line-height:$floatBarBox_height;
        border-bottom:1px solid #edf1f2;
        cursor: pointer;
        overflow: hidden;
        position: relative;
        &:hover{
            .first-box{
                margin-top: -$floatBarBox_height;
            }
        }

        .first-box,.hidden-box{
            width: $floatBarBox_height;
            height: $floatBarBox_height;
            @extend %transition-03;
        }
        .first-box{
            i{
                font-style: normal;
                font-size: 24px;
            }
        }
        .hidden-box{
            background-color: $main_color;
            color:#FFF;
            @include box-shadow(1px 2px 2px rgba(0,0,0,.2));
        }


    }
}

