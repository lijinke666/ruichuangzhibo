<script>
    $(function(){
        var $remove_video = $('.remove-class');
        $remove_video.on('click',function(){
            var $this = $(this),
                $id = $this.data('id'),
                rc;
            rcConfirm('确定要删除本课程吗?',function( index ){
                if( index ==1 ){
                    $.ajax({
                        url:"",
                        type:"get",
                        data:{id:$id},
                        dataType:"json",
                        beforeSend:function(){
                            rc = new rcLoading('处理中...');
                        },
                        success:function(){
                            rc.remove();
                            rcAlert('删除成功',true)
                        },
                        error:function(){
                            rc.remove();
                            rcAlert('删除失败,请刷新重试',false);
                        }
                    })
                }
            },{'确定':'btn-primary','取消':'btn-cancel'})
        })
    })
</script>
