		<script src="newJs/jquery-1.11.3.js"></script>
		<script src="newJs/script.js"></script>
		<script>
			//标题进入动画
			function add_title_animate(ele, animateName) {
				ele.addClass(animateName);
			}

			//队列动画效果
			function toAnimate(ele, animateName) {
				var course_title_list = ele,
						course_title_list_size = course_title_list.size();
				var Index = 0;
				var listSet = setInterval(function () {
					course_title_list.eq(Index).addClass(animateName);
					Index++;
					if (Index == course_title_list_size) {
						clearInterval(listSet)
					}
				}, 30)
			}
			$(function() {
				var PicIndex = 0;   //图片索引
				var bannerPic;       //计时器
				var $banner = $(".banner"),
					$bannerMove = $(".banner-move"),
					$banner_move = $bannerMove.find("li"),
					$banner_list = $(".banner-list"),
					$banner_width = $banner_move.width(),
					$banner_length = $banner_move.size(),
					$arrow = $(".arrow-box"),
					$left_arrow = $arrow.find(".left-arrow"),     //左切换按钮
					$right_arrow = $arrow.find(".right-arrow");    //右切换按钮

				//动态加载小圈圈
				createList($banner_move);
				initBannerWidth($bannerMove);
				//幻灯片切换
				move(0);
				function move(Index) {
					var a = $banner_list.find("a");
					$banner.addClass("animation");
					$banner_move.animate({"left": -Index * $banner_width});
					a.eq(Index).addClass("on").siblings().removeClass("on");
					$banner_move.eq(Index).find(".banner-move-item>h2")
							.addClass("banner-item-title-animateDown")
							.parent().parent().siblings().find(".banner-move-item>h2")
							.removeClass("banner-item-title-animateDown");
					$banner_move.eq(Index).find(".banner-move-item>p")
							.addClass("banner-item-title-animateUp").parent().parent().siblings().find(".banner-move-item>p")
							.removeClass("banner-item-title-animateUp");
					$banner_move.eq(Index).find(".banner-move-item>a")
							.addClass("banner-item-a-animate").parent().parent().siblings().find(".banner-move-item>a")
							.removeClass("banner-item-a-animate");
				}

				//初始化轮播宽度
				function initBannerWidth(ele) {
					ele.css({
						"width": $banner_length * $banner_width
					})
				}

				//动态加载索引点
				function createList(ele) {
					$.each(ele, function (i) {
						var aNode = '';
						if (i == 0) {
							aNode = $("<a href='javascript:void(0)' class='on'></a>");
						} else {
							aNode = $("<a href='javascript:void(0)'></a>");
						}
						$banner_list.append(aNode);
					});
				}

				//计时轮播
				$banner.hover(function () {
					clearInterval(bannerPic);
				}, function () {
					bannerPic = setInterval(function () {
						if (PicIndex == $banner_length - 1) {
							PicIndex = 0;
						} else {
							PicIndex++;
						}
						move(PicIndex);
					}, 6000);
				}).trigger("mouseleave");

				//点击索引点切换轮播图
				$banner_list.on("click", "a", function () {
					var list_a = $banner_list.find("a"),
							index = list_a.index(this),
							$this = $(this);
					$this.addClass("on").siblings().removeClass("on");
					move(index)
				});

				//向右切换
				$right_arrow.on("click", function () {
					if (PicIndex == $banner_length - 1) {
						PicIndex = 0;
					} else {
						PicIndex++;
					}
					move(PicIndex)
				});

				//向左切换
				$left_arrow.on("click", function () {
					if (PicIndex == 0) {
						PicIndex = $banner_length - 1;
					} else {
						PicIndex--;
					}
					move(PicIndex)
				});


				//科目 初高中切换
				var $level_toggle = $(".level-toggle > .select-level"),
						$course_title_list = $(".course-title-list:eq(0)");
				$level_toggle.on("click", function () {
					var $this = $(this);
					$this.addClass("on").siblings().removeClass("on");
				});
				$level_toggle.eq(0).on("click", function () {
					$course_title_list.removeClass("translate")
				}).next().on("click", function () {
					$course_title_list.addClass("translate")
				});

				//页面滚动动画
				var $win = $(window);
				$win.on("scroll", function () {
					var $scrollTop = $(document).scrollTop(),
						$documentHeight = $(document).height(),
						$windowHeight = $(window).height(),
						$study_experience = $(".study-experience"),
						$study_experience_phone = $(".study-experience-phone"),
						$study_experience_a = $(".study-experience-a"),
						$course_title = $(".course-title"),
						$course_title_list = $(".course-title-list"),
						$score_program = $(".score-program"),
						$all_apps_title = $(".all-apps-title"),
						$interaction = $(".interaction");

					var $study_experience_infoOffsetTop = $study_experience.offset().top,
							$course_titleOffsetTop = $course_title_list.offset().top,
							$score_programTop = $score_program.offset().top,
							$all_apps_titleOffsetTop = $all_apps_title.offset().top,
							$interactionOffsetTop = $interaction.offset().top;

					if ($windowHeight + $scrollTop >= $documentHeight) {
						$win.off("scroll");
						return;
					}

					if( $scrollTop>= $interactionOffsetTop -350 ){
						add_title_animate($interaction.find(".interaction-imgZindex1"),"interaction-img1-animate");
						add_title_animate($interaction.find(".interaction-imgZindex2"),"interaction-img2-animate");
						add_title_animate($interaction.find(".interaction-text"),"banner-item-p-animate");
					}

					if ($scrollTop >= $score_programTop - 320) {
						add_title_animate($score_program.find("h2"), "banner-item-title-animateDown");
						add_title_animate($score_program.find("p"), "banner-item-title-animateFade");
						$score_program.find(".item").eq(0).addClass("score-animate-left");
						$score_program.find(".item").eq(1).addClass("score-animate-center");
						$score_program.find(".item").eq(2).addClass("score-animate-right");
					}

					if ($scrollTop >= $study_experience_infoOffsetTop - 400) {
						toAnimate($study_experience.find("h2"), "banner-item-h2-animate");
						add_title_animate($study_experience.find("p"), "banner-item-title-animateFade");
						add_title_animate($study_experience_a, "banner-item-title-animateUp");
						add_title_animate($study_experience_phone, "phone-animate");
						add_title_animate($study_experience, "gradient-move");

					}

					if ($scrollTop >= $course_titleOffsetTop - 700) {
						add_title_animate($course_title.find("h2"), "banner-item-title-animateDown");
						add_title_animate($course_title.find("p"), "banner-item-title-animateFade");
						toAnimate($course_title_list.find("li"), "toScale");
					}


					if ($scrollTop >= $all_apps_titleOffsetTop - 700) {
						add_title_animate($all_apps_title.find("h2"), "banner-item-title-animateDown");
						add_title_animate($all_apps_title.find("p"), "banner-item-title-animateFade");
						add_title_animate($all_apps_title.find(".item"), "app-animate");
						add_title_animate($all_apps_title.find("a"), "banner-item-title-animateUp");
					}
				});

			});
		</script>
